    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.42.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
